        <android.support.v7.widget.LinearLayoutCompat
            android:id="@+id/titleSoccerFragment"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="5dp"
            android:background="@color/soccerColor"
            android:paddingBottom="10dp"
            android:paddingTop="10dp"
            android:visibility="gone">

            <com.ready.sport.inmatch.util.TextViewPlus
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/soccer_label"
                android:textAlignment="center"
                android:textColor="#ffff"
                android:textSize="22dp" />
        </android.support.v7.widget.LinearLayoutCompat>

        <android.support.v7.widget.LinearLayoutCompat
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:paddingTop="20dp">

            <com.ready.sport.inmatch.util.CircularProgressBar
                android:id="@+id/circularprogressbarSoccer"
                style="@style/Widget.ProgressBar.Soccer.CircularProgressBar"
                android:layout_width="120dip"
                android:layout_height="120dip"
                android:layout_gravity="center"
                android:layout_marginBottom="20dp"
                android:layout_marginTop="10dip"
                app:title="5,0" />
        </android.support.v7.widget.LinearLayoutCompat>

        <android.support.v7.widget.LinearLayoutCompat
            android:layout_width="match_parent"
            android:layout_height="60dp">

            <com.ready.sport.inmatch.util.TextViewPlus
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:paddingLeft="20dp"
                android:text="@string/role" />

            <android.support.v7.widget.AppCompatSpinner
                android:id="@+id/spinnerSoccer"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:paddingLeft="10dp"
                android:background="@drawable/spinner_layout"
                android:gravity="center"
                android:spinnerMode="dialog"
                android:textAlignment="center"
                >

            </android.support.v7.widget.AppCompatSpinner>
        </android.support.v7.widget.LinearLayoutCompat>

