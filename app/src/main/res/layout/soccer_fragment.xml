        <SeekBar
            android:id="@+id/seekBarTest"
            style="@style/Widget.AppCompat.SeekBar.Discrete"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:backgroundTint="@color/colorButton"
            android:background="@color/colorButton"
            android:max="100"
            android:progress="1" />

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/separatorColor" />

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="30dp">

            <iammert.com.huelib.HueSeekBar
                android:id="@+id/seekbarSocVel"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                app:bottomSeekHeight="20dp"
                app:bottomSeekToggleWidth="20dp"
                app:colorEmpty="@color/colorButton"
                app:colorFill="@color/colorPrimary"
                app:colorToggle="@color/colorSecond"
                app:maxProgress="100"
                app:currentProgress="50"/>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="60dp">

                <com.ready.sport.inmatch.util.TextViewPlus
                    android:id="@+id/seekbarSocVelText"
                    android:layout_width="150dp"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_gravity="center"
                    android:paddingLeft="20dp"
                    android:text="@string/velocity" />

                <com.ready.sport.inmatch.util.TextViewPlus
                    android:id="@+id/seekbarSocVelNum"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_gravity="center"
                    android:layout_marginLeft="16dp"
                    android:layout_toRightOf="@+id/seekbarSocVelText"
                    android:text="5.0 / 10"
                    android:textAlignment="center"
                    android:textSize="20dp"
                    android:textStyle="bold" />
            </RelativeLayout>
        </RelativeLayout>

